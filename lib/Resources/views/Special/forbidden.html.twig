{% extends ["::page.html.twig", "AppBundle:Include:page.html.twig", "AgitUiBundle:Include:page.html.twig"] %}

{% agit.capability "" %}
{% agit.status "403" %}
{% block name %}{{ t("Forbidden") }}{% endblock %}

{% block main %}
    <main>
        <div class="callout callout-warning">{{ t("You are not allowed to access this page. Please contact an administrator to upgrade your privileges.") }}</div>
    </main>
{% endblock %}
